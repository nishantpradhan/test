

function isIsomorphic = (s,t){
	if(s.length !== t.length){
		return false 
	}


	let mapS = Array(256).fill(-1)

		let mapT = Array(256).fill(-1)

		for(let i = 0 ; i < s.length;i++){
			let charT = t.charCodeAt(i);

			let charS = s.charCodeAt(i);

			if(mapT[charT] === -1 && mapS[charS] === -1){
				map[charT] = charS;

				map[charS] = charT;   
			}else if (mapT[charT] !== charS || mapS[charS] !== charT){
				return false;
			}


		}


	return true;

}
